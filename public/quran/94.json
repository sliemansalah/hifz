{"number":94,"name":"سُورَةُ الشَّرۡحِ","englishName":"Ash-Sharh","numberOfAyahs":8,"ayahs":[{"number":6091,"text":"بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ أَلَمْ نَشْرَحْ لَكَ صَدْرَكَ","numberInSurah":1,"juz":30,"page":596},{"number":6092,"text":"وَوَضَعْنَا عَنكَ وِزْرَكَ","numberInSurah":2,"juz":30,"page":596},{"number":6093,"text":"ٱلَّذِىٓ أَنقَضَ ظَهْرَكَ","numberInSurah":3,"juz":30,"page":596},{"number":6094,"text":"وَرَفَعْنَا لَكَ ذِكْرَكَ","numberInSurah":4,"juz":30,"page":596},{"number":6095,"text":"فَإِنَّ مَعَ ٱلْعُسْرِ يُسْرًا","numberInSurah":5,"juz":30,"page":596},{"number":6096,"text":"إِنَّ مَعَ ٱلْعُسْرِ يُسْرًۭا","numberInSurah":6,"juz":30,"page":596},{"number":6097,"text":"فَإِذَا فَرَغْتَ فَٱنصَبْ","numberInSurah":7,"juz":30,"page":596},{"number":6098,"text":"وَإِلَىٰ رَبِّكَ فَٱرْغَب","numberInSurah":8,"juz":30,"page":596}]}